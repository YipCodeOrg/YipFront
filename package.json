    "@storybook/testing-library": "^0.0.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "env-cmd": "^10.1.0",
    "madge": "^5.0.1",
    "react-scripts": "^5.0.1",
    "webpack": "5"
  },
  "overrides": {
    "react-refresh": "0.14.0"
  },
  "scripts": {
    "start": "env-cmd -r .env-cmdrc -e local, react-scripts start",
    "build:local": "env-cmd -r .env-cmdrc -e local, react-scripts build",
    "build:prod": "env-cmd -r .env-cmdrc -e prod, react-scripts build",
    "build:dev": "env-cmd -r .env-cmdrc -e dev, react-scripts build",
    "test:local": "env-cmd -r .env-cmdrc -e local, react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
